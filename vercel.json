{
    "version": 2,
    "builds": [
      {
        "src": "./server/index.js",
        "use": "@vercel/node",
        "config": {}
      }
    ],
    "routes": [
      {
        "src": "/api/*",
        "dest": "/api"
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }]
  }